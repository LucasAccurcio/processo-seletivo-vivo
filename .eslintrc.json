{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "no-console": "off",
        "object-curly-newline": "off",
        "no-undef": "off"
    }
}
